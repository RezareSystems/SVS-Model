                    "        private void OnSCRUMTotalNDemand(object sender, ScrumFertDemandData sfdd)",
                    "        {",
                    "        \tif((sfdd.FertDuration>0)&&(sfdd.TotalNDemand>0))",
                    "        \t{",
                    "\t\t    \tint splitLength = (int) sfdd.FertDuration/Splits;",
                    "\t\t    \tdouble splitN = (sfdd.TotalNDemand * RelativeFert)/Splits;",
                    "\t\t    \tApplicationDates = new Dictionary<DateTime,double>();",
                    "\t\t    \tfor (int i = 0; i < Splits; i++)",
                    "\t\t    \t{",
                    "\t\t    \t\tDateTime nextAppn = clock.Today.AddDays(sfdd.NonFertDuration).AddDays(i*splitLength);",
                    "\t\t    \t\tApplicationDates.Add(nextAppn,splitN);",
                    "\t\t    \t}",
                    "        \t}",
                    "        }",
