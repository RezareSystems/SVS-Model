                    "        {",
                    "        \tif (ApplicationDates != null)",
                    "        \t{",
                    "\t\t    \tforeach(KeyValuePair<DateTime, double> entry in ApplicationDates)",
                    "\t\t\t\t\t{",
                    "\t\t\t\t\t\tif (entry.Key == clock.Today)",
                    "\t\t\t\t\t\t{",
                    "\t\t\t\t\t\t\t  fertiliser.Apply(entry.Value, Type: FertiliserType);",
                    "\t\t\t\t\t\t}",
